{
  "name": "svelte-app",
  "version": "1.0.1",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rollup": "^1.12.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "svelte": "^3.0.0"
  },
  "scripts": {
    "build": "rollup -c; hugo",
    "autobuild": "rollup -c -w",
    "dev1": "run-p autobuild hugo:dev1",
    "dev2": "run-p autobuild hugo:dev2",
    "hugo:dev1": "hugo server",
    "hugo:dev2": "hugo server --renderToDisk --cleanDestinationDir"
  },
  "dependencies": {
    "svelte-spa-router": "^3.1.0"
  }
}
